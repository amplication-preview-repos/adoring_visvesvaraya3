datasource db {
  provider = "postgresql"
  url      = env("DB_URL")
}

generator client {
  provider = "prisma-client-js"
}

model Button {
  color     EnumButtonColor?
  createdAt DateTime         @default(now())
  id        String           @id @default(cuid())
  text      String?
  updatedAt DateTime         @updatedAt
}

enum EnumButtonColor {
  Option1
}
